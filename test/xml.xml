<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MODE SYSTEM "file:/home/okaasan/src/jsxe/test/xmode.dtd">
<MODE>
  <PROPS>
    <PROPERTY NAME="commentStart" VALUE="&lt;!--"/>
    <PROPERTY NAME="commentEnd" VALUE="--&gt;"/>
  </PROPS>
  <RULES>
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>&lt;!--</BEGIN>
      <END>--&gt;</END>
    </SPAN>
    <!-- DTD Entity declarations -->
    <SPAN DELEGATE="ENTITY-TAGS" TYPE="KEYWORD2">
      <BEGIN>&lt;!ENTITY</BEGIN>
      <END>&gt;</END>
    </SPAN>
    <!-- CDATA sections -->
    <SPAN DELEGATE="CDATA" TYPE="KEYWORD2">
      <BEGIN>&lt;![CDATA[</BEGIN>
      <END>]]&gt;</END>
    </SPAN>
    <!-- DTD instructions -->
    <SPAN DELEGATE="DTD-TAGS" TYPE="KEYWORD2">
      <BEGIN>&lt;!</BEGIN>
      <END>&gt;</END>
    </SPAN>
    <!-- PI directives -->
    <SPAN TYPE="KEYWORD3">
      <BEGIN>&lt;?</BEGIN>
      <END>&gt;</END>
    </SPAN>
    <!-- markup tags -->
    <SPAN DELEGATE="TAGS" TYPE="MARKUP">
      <BEGIN>&lt;</BEGIN>
      <END>&gt;</END>
    </SPAN>
    <!-- SGML entities -->
    <SPAN NO_WORD_BREAK="TRUE" TYPE="LITERAL2">
      <BEGIN>&amp;</BEGIN>
      <END>;</END>
    </SPAN>
  </RULES>
  <RULES DEFAULT="MARKUP" NO_WORD_SEP="-_" SET="TAGS">
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>&lt;!--</BEGIN>
      <END>--&gt;</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&quot;</BEGIN>
      <END>&quot;</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&apos;</BEGIN>
      <END>&apos;</END>
    </SPAN>
    <SEQ TYPE="MARKUP">/</SEQ>
    <MARK_PREVIOUS EXCLUDE_MATCH="TRUE" TYPE="LABEL">:</MARK_PREVIOUS>
    <SEQ TYPE="OPERATOR">:</SEQ>
  </RULES>
  <RULES DEFAULT="KEYWORD2" IGNORE_CASE="FALSE" SET="DTD-TAGS">
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>&lt;!--</BEGIN>
      <END>--&gt;</END>
    </SPAN>
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>--</BEGIN>
      <END>--</END>
    </SPAN>
    <!-- Parameter entities -->
    <SPAN NO_WORD_BREAK="TRUE" TYPE="LITERAL2">
      <BEGIN>%</BEGIN>
      <END>;</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&quot;</BEGIN>
      <END>&quot;</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&apos;</BEGIN>
      <END>&apos;</END>
    </SPAN>
    <!-- This appears in some SGML documents -->
    <SPAN DELEGATE="MAIN" TYPE="KEYWORD2">
      <BEGIN>[</BEGIN>
      <END>]</END>
    </SPAN>
    <SEQ TYPE="OPERATOR">(</SEQ>
    <SEQ TYPE="OPERATOR">)</SEQ>
    <SEQ TYPE="OPERATOR">|</SEQ>
    <SEQ TYPE="OPERATOR">?</SEQ>
    <SEQ TYPE="OPERATOR">*</SEQ>
    <SEQ TYPE="OPERATOR">+</SEQ>
    <SEQ TYPE="OPERATOR">,</SEQ>
    <KEYWORDS>
      <KEYWORD1>CDATA</KEYWORD1>
      <KEYWORD1>EMPTY</KEYWORD1>
      <KEYWORD1>INCLUDE</KEYWORD1>
      <KEYWORD1>IGNORE</KEYWORD1>
      <KEYWORD1>NDATA</KEYWORD1>
      <KEYWORD1>#IMPLIED</KEYWORD1>
      <KEYWORD1>#PCDATA</KEYWORD1>
      <KEYWORD1>#REQUIRED</KEYWORD1>
    </KEYWORDS>
  </RULES>
  <RULES DEFAULT="KEYWORD2" SET="ENTITY-TAGS">
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>&lt;!--</BEGIN>
      <END>--&gt;</END>
    </SPAN>
    <!-- SGML comment -->
    <SPAN TYPE="COMMENT1">
      <BEGIN>--</BEGIN>
      <END>--</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&quot;</BEGIN>
      <END>&quot;</END>
    </SPAN>
    <SPAN NO_LINE_BREAK="TRUE" TYPE="LITERAL1">
      <BEGIN>&apos;</BEGIN>
      <END>&apos;</END>
    </SPAN>
    <SEQ TYPE="OPERATOR">=</SEQ>
    <SEQ TYPE="OPERATOR">%</SEQ>
    <KEYWORDS>
      <KEYWORD1>SYSTEM</KEYWORD1>
    </KEYWORDS>
  </RULES>
  <RULES DEFAULT="COMMENT2" SET="CDATA">
    <!-- no rules -->
  </RULES>
</MODE>